<UserControl x:Class="ServerDashboard.OEEControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ServerDashboard"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center">Overall Equipment Effectiveness (OEE)</Label>
        <Label Grid.Row="1" Grid.Column="0" HorizontalAlignment="Center">RaspberryPi-1</Label>
        <Label Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center">RaspberryPi-2</Label>
        <Label Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" x:Name="OEELabel1">Number</Label>
        <Label Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" x:Name="OEELabel2">Number</Label>
    </Grid>
</UserControl>
